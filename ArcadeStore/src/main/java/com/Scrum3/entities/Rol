package com.postgres.ArcadeStore.entities;

import jakarta.persistence.*;
import lombok.allArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Entity
@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
@Table(name = "tb_roles")
public class Rol {
@ID
@GenerateValue(strategy = GenerationType.IDENTITY)
@Column(name = "idRol")
private long id;
@Colum(name = "name", length = 50)
private String name;
private String description;